{
  "name": "purrplingbot",
  "version": "1.4.0-dev",
  "codename": "Catnip",
  "description": "A discordbot written primary for server PurrplingCat",
  "main": "purrplingbot.js",
  "scripts": {
    "depmod": "npm-recursive-install --skip-root",
    "clean": "/bin/sh -c 'find . -name \"node_modules\" -type d -prune -exec rm -rf \"{}\" \\; ; find . -name \"package-lock.json\" -type f -exec rm \"{}\" \\;'",
    "start": "nodemon --exitcrash --watch config/ purrplingbot.js 2>&1 3>&1 | tee -a purrplingbot.log",
    "test": "ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EllenFawkes/PurrplingBot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "twitch",
    "chat",
    "nodejs",
    "javascript"
  ],
  "author": "PurrplingCat",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/EllenFawkes/PurrplingBot/issues"
  },
  "homepage": "https://github.com/EllenFawkes/PurrplingBot#readme",
  "dependencies": {
    "console-stamp": "^0.2.5",
    "discord.js": "^11.2.0",
    "js-yaml": "^3.10.0",
    "merge": "^1.2.0",
    "moment": "^2.18.1",
    "nodemon": "^1.12.0",
    "npm": "",
    "recursive-install": "^1.3.0",
    "request": "^2.81.0",
    "semaphore": "^1.1.0",
    "twix": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "bluebird": "^3.5.0",
    "cross-env": "^5.0.1",
    "debug": "^3.0.1",
    "imurmurhash": "^0.1.4",
    "jshint": "^2.9.5",
    "jslint": "^0.11.0"
  },
  "nodemonConfig": {
    "ignore": [
      "config/*db.*",
      "config/storage.*",
      "config/*.bk"
    ],
    "ext": "js json yaml yml"
  }
}
