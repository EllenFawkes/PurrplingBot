{
  "name": "purrplingbot",
  "version": "1.3.0-beta3-dev",
  "codename": "Carmilla",
  "description": "A discordbot written primary for server PurrplingCat",
  "main": "purrplingbot.js",
  "scripts": {
    "depmod": "./node_modules/recursive-install/recursive-install.js --skip-root",
    "clean": "/bin/sh -c 'find . -name \"node_modules\" -type d -prune -exec rm -rf \"{}\" \\; ; find . -name \"package-lock.json\" -type f -exec rm \"{}\" \\;'",
    "start": "node purrplingbot.js 2>&1 3>&1 | tee -a purrplingbot.log",
    "test": "cross-env NODE_ENV=test PORT=8080 node_modules/.bin/ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EllenFawkes/PurrplingBot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "twitch",
    "chat",
    "nodejs",
    "javascript"
  ],
  "author": "PurrplingCat",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/EllenFawkes/PurrplingBot/issues"
  },
  "homepage": "https://github.com/EllenFawkes/PurrplingBot#readme",
  "dependencies": {
    "console-stamp": "^0.2.5",
    "request": "^2.81.0",
    "discord.js": "^11.1.0",
    "js-yaml": "^3.10.0",
    "moment": "^2.18.1",
    "npm": "",
    "recursive-install": "^1.3.0"
  },
  "devDependencies": {
    "cross-env": "^5.0.1"
  }
}
